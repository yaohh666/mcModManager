<!--pages/search/search.wxml-->
<!--pages/search/search.wxml-->
<view class='search'>
  <input type='text' placeholder='请输入您要搜索的内容' bindinput='input' bindconfirm='confirm' confirm-type="search"/>
  <icon type='search' class='icons'></icon>
  <view wx:for="{{list}}"  wx:key='' class='lists'>
    <text class="list-item" wx:if="{{item.show}}">{{item.name}}</text>
  </view>
</view>

